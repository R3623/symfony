{% extends 'base.html.twig' %}

{% block title %}Liste des Événements{% endblock %}

{% block body %}
    <h2>Liste des Événements</h2>

    <ul>
        {% for event in events %}
            <li>{{ event.title }} - {{ event.date|date('d/m/Y') }}</li>
        {% else %}
            <li>Aucun événement trouvé.</li>
        {% endfor %}
    </ul>
{% endblock %}
